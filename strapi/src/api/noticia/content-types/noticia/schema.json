{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "singularName": "noticia",
    "pluralName": "noticias",
    "displayName": "Noticia"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "contenido": {
      "type": "blocks"
    },
    "imagen_destacada": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "fecha_publicacion": {
      "type": "date"
    },
    "etiquetas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::etiqueta.etiqueta",
      "inversedBy": "noticias"
    },
    "Destacado": {
      "type": "boolean"
    }
  }
}
